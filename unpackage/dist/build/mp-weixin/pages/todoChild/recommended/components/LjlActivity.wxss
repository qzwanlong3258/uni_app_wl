.activity-box{display:-webkit-box;display:-webkit-flex;display:flex;width:100%;height:170rpx;box-sizing:border-box;padding:24rpx 30rpx}.activity-box .left{width:250rpx;line-height:110rpx;color:#fad64c;font-size:34rpx;background:#d8d8d8;text-align:center}.activity-box .right{box-sizing:border-box;padding:0 24rpx;width:calc(100% - 250rpx);line-height:110rpx;font-size:34rpx;font-weight:500;color:#333;overflow:hidden;text-overflow:elipsis;white-space:nowarp}
.no-more-data-box {
  width: 100%;
  height: 50rpx;
  background-color: #666666;
  text-align: center;
  overflow: hidden;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
}
.no-more-data-box text {
    color: #FFFFFF;
    font-size: 26rpx;
}
@-webkit-keyframes no-data {
80% {
    height: 50rpx;
}
100% {
    height: 0;
}
}
@keyframes no-data {
80% {
    height: 50rpx;
}
100% {
    height: 0;
}
}


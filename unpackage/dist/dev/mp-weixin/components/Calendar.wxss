
.all {
	
	background: #FFFFFF;
}
.all .bd image{
	width: 30rpx;
}
.all .bar {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 0 10rpx;
	/* padding: 10rpx; */
}
.bar .barbtn {
	height: 60rpx;
	line-height: 60rpx;
	font-size: 24rpx;
}
.all .week {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 20rpx 20rpx 0 20rpx;
	padding-left: 40rpx;
	padding-right: 40rpx;
	margin: 20rpx 33rpx 0 33rpx;
	border-radius: 20rpx;
	background-color: #fff;
	font-weight: 500;
}
.myDateTable {
	margin:0 2.5vw 2.5vw 2.5vw;
	padding:0 2vw 2vw 2vw;
	border-radius: 20rpx;
	background: #FFFFFF;
}
.myDateTable .dateCell {
		width: 11vw;
		padding: 1vw;
		display: inline-block;
		text-align: center;
		font-size: 32rpx;
}
.dateCell .cell {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		border-radius: 50%;
		height: 11vw;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.whiteColor {
	color: #fff;
}
.blackColor{
	color: #000000;
}
.greenColor {
	color: #01b90b;
	font-weight: bold;
}
.bgWhite {
	background-color: #FDE40B;
}
.bgGray {
	background-color: rgba(255, 255, 255, 0.42);
}
.bgBlue {
	font-size: 14px;
	background-color: #4b95e6;
}
.bgYellow{
	font-size: 14px;
	background-color: #FDE40B;
}
.bgRed{
	font-size: 14px;
	background-color: #FF853E;
	font-family:FZLTKHK;
	text-shadow:0px 3px 5px rgba(100,2,3,0.39);
}
.redColor {
	color: #ff0000;
}
.TipArea{
	word-break:break-all;
	word-wrap:break-word;
	
	font-size: 14px;
	padding: 10px;
}
.impTip{
	display: inline-block;
	color: #ff0000;
}


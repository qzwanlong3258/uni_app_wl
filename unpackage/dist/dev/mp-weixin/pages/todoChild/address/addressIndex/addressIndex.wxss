
.form-list {
	width: 100%;
	background: #fff;
}
.form-list .list-column {
	width: 100%;
	min-height: 180rpx;
	border-bottom: 1rpx solid #eee;
}
.list-column .column-top {
	width: 100%;
	height: 80rpx;
	font-size: 32rpx;
	line-height: 80rpx;
	display: -webkit-box;
	display: flex;
	display: -webkit-flex;
	padding-left: 20rpx;
}
.column-top .column-phone {
	font-size: 24rpx;
	color: #666;
	margin-left: 20rpx;
	line-height: 90rpx;
}
.list-column .column-bottom {
	width: 710rpx;
	margin-left: 20rpx;
	display: -webkit-flex;
	font-size: 28rpx;
}
.list-column .default-logo {
	width: 80rpx;
	height: 42rpx;
	background: #000000;
	border-radius: 30rpx;
	text-align: center;
	line-height: 40rpx;
	color: #fff;
	margin-top: 14rpx;
	margin-right: 30rpx;
}
.list-column .column-address {
	width: 65%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	overflow: hidden;
	text-overflow: ellipsis;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	min-height: 70rpx;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.list-column .column-exit {
	width: 95rpx;
	height: 40rpx;
	border-left: 1rpx solid #eee;
	text-align: right;
	line-height: 40rpx;
	position: absolute;
	right: 30rpx;
	font-size: 30rpx;
}
.default-image {
	width: 400rpx;
	height: 300rpx;
	margin: auto;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	position: absolute;
}
.default-image image {
	width: 100%;
	height: 100%;
}


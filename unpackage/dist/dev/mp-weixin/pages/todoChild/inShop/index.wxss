.w-picker {
  position: relative;
  z-index: 888;
}
.w-picker .mask {
    position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
    visibility: hidden;
    opacity: 0;
    -webkit-transition: all 0.3s ease;
    transition: all 0.3s ease;
}
.w-picker .mask.show {
    visibility: visible;
    opacity: 1;
}
.w-picker .w-picker-cnt {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    -webkit-transition: all 0.3s ease;
    transition: all 0.3s ease;
    -webkit-transform: translateY(100%);
            transform: translateY(100%);
    z-index: 3000;
}
.w-picker .w-picker-cnt.show {
    -webkit-transform: translateY(0);
            transform: translateY(0);
}
.w-picker .w-picker-hd {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    padding: 0 30rpx;
    height: 88rpx;
    background-color: #fff;
    position: relative;
    text-align: center;
    font-size: 32rpx;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.w-picker .w-picker-hd .w-picker-btn {
      font-size: 30rpx;
}
.w-picker .w-picker-hd:after {
    content: ' ';
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 100%;
            transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
            transform: scaleY(0.5);
}
.w-picker .w-picker-item {
    text-align: center;
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 30rpx;
}
.w-picker .w-picker-view {
    width: 100%;
    height: 476rpx;
    overflow: hidden;
    background-color: white;
    z-index: 666;
}
.w-picker picker-view {
    height: 100%;
}



.container {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
  position: absolute;
  overflow: scroll;
}
/*导航下划线*/
.border-active {
  border-bottom: 5rpx solid #df365d;
}
/*点击改变区块样式*/
.hover-active {
  background: #000000 !important;
  color: #f2f2f2 !important;
}
/*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
/*原生button样式*/
button {
  background-color: #fafafe;
}
button::after {
  border: none;
  background: none;
}
/*底部提交按钮位置*/
.start-todo {
  position: fixed;
  bottom: 40rpx;
  margin: 0 auto;
  right: 0;
  left: 0;
}
/*自定义button的样式*/
.customize-button {
  border-radius: 42rpx;
  color: #fff;
  background: #000;
  text-align: center;
}
/*页面大按钮主色调*/
.cus-big-button {
  width: 690rpx;
  height: 80rpx;
  font-size: 34rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 48rpx;
  color: #fff;
  background: -webkit-linear-gradient(315deg, #46d0fe, #409eff);
  background: linear-gradient(135deg, #46d0fe, #409eff);
  margin-left: 30rpx;
}
/*页面大按钮副色调*/
.primary-cus-big-button {
  width: 690rpx;
  height: 80rpx;
  font-size: 34rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 48rpx;
  color: #fff;
  background: -webkit-linear-gradient(315deg, #df365d, #ee141e);
  background: linear-gradient(135deg, #df365d, #ee141e);
  margin-left: 30rpx;
}
/*底部覆盖副按钮*/
.primary-cus-button {
  width: 100%;
  height: 100rpx;
  font-size: 34rpx;
  text-align: center;
  line-height: 100rpx;
  color: #fff;
  background: -webkit-linear-gradient(315deg, #df365d, #ee141e);
  background: linear-gradient(135deg, #df365d, #ee141e);
}
/*背景图*/
.back-ground-set {
  background-size: 100% 100% !important;
  background-repeat: no-repeat !important;
}
/*主题色*/
.theme-color {
  color: #FAD64C;
}
.primary-theme-color {
  color: #FAD64C;
}
/*字体主颜色*/
.primary-text-color {
  color: #404040;
}
/*副色调*/
.conllect-color {
  color: #e64340;
}
/*重要级文字信息*/
.important-font {
  color: #1e1d22;
  font-size: 32rpx;
  font-weight: bold;
}
/*标题字体颜色*/
.title-font {
  font-size: 32rpx;
  color: #404040;
}
/*内容文字*/
.content-font {
  font-size: 28rpx;
}
/* 按钮属性 */
.type-btn-font {
  font-size: 26rpx;
}
/*提示性文字：如好评人数*/
.tip-font {
  color: #8f8fa7;
  font-size: 24rpx;
}
/*阴影*/
.box-shadow-setting {
  box-shadow: 0 0 6rpx 6rpx rgba(0, 0, 0, 0.1);
}
/*列表底部留白高度*/
.white-height {
  height: 120rpx;
}
/* 黑色透明蒙板 */
.transparent-background {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  top: 0;
}
/* 脱离文档流 */
.out-document {
  position: absolute;
  z-index: 1;
}
/*单行省略号*/
.one-line-elipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
/** border主题色线条 */
.border-theme-color {
  border: 1rpx solid #e8be2e;
}
.border-primary-theme-color {
  border: 1rpx solid #df365d;
}
/**border灰色线条*/
.border-primary-color {
  border: 1rpx solid #ddd;
}
/*有框的输入框样式*/
.type-input {
  border: 1rpx solid #ddd;
  height: 70rpx;
  border-radius: 30rpx;
  padding-left: 20rpx;
}
/*空白页样式设计*/
.null-data {
  width: 50%;
  height: 300rpx;
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  top: 0;
  bottom: 130rpx;
}
/*对checkedbox的样式修改*/
/* checkbox未选中时样式 */
checkbox .wx-checkbox-input {
  border-radius: 50%;
  height: 36rpx;
  width: 36rpx;
}
/* checkbox选中时样式 */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background-color: #409eff;
  border: 1rpx solid #409eff;
  /* 自定义样式.... */
  color: #fff !important;
}
/*对微信radios的样式修改*/
radio .wx-radio-input {
  border-radius: 50%;
  /* 圆角 */
  width: 36rpx;
  height: 36rpx;
}
/* 选中后的 背景样式 （红色背景 无边框 可根据UI需求自己修改） */
radio .wx-radio-input.wx-radio-input-checked {
  border: none;
  background: #409eff !important;
}
/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 50%;
  /* 圆角 */
  width: 36rpx;
  /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 36rpx;
  /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 36rpx;
  text-align: center;
  font-size: 30rpx;
  /* 对勾大小 30upx */
  color: #fff;
  /* 对勾颜色 白色 */
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
